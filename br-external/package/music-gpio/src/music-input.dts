/dts-v1/;
/plugin/;

/ {
    compatible = "brcm,bcm2711";
    
    fragment@0 {
        target-path = "/";
        __overlay__ {
            music_input {
                compatible = "music-input-device";
                btn-gpios = <&gpio 17 1>;
                status = "okay";
            };
        };
    };
};
